[core]
    editor = vim
[push]
    default = current
[diff]
    tool = vimdiff
[merge]
    tool = vimdiff
[alias]
    a = add -A
    ap = add --patch
    b = branch
    c = commit
    ca = commit --amend
    co = checkout
    d = diff
    dc = diff --cached
    dt = difftool
    dr = diff @ @{u}
    l = log --oneline --decorate --graph
    pr = pull --rebase
    rh = reset --hard
    rr = !git stash && git stash drop
    rs = reset --soft
    rf = reflog
    ri = rebase -i
    rso = remote show origin
    s = status -bs
